<?xml version="1.0" encoding="utf-8"?>
<nl.sebastiaanschool.contact.app.TeamMemberView xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/ListItem"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/team__displayName"
        style="@style/ListItemTitle"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" />

    <TextView
        android:id="@+id/team__detailText"
        style="@style/ListItemDetail"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" />
</nl.sebastiaanschool.contact.app.TeamMemberView>